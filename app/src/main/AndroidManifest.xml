<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.temple.selectionactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/apk_icon"
        android:roundIcon="@mipmap/apk_icon_round"
        android:supportsRtl="true"
        android:label="@string/apk_label"
        android:theme="@style/Theme.SelectionActivity">
        <activity
            android:name=".SelectionActivity"
            android:exported="true"
            android:label="@string/app_title_label">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisplayActivity"
            android:exported="true"
            android:label="@string/display_title_label"/>
    </application>

</manifest>